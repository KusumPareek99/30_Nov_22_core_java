  public static boolean isCyclicSubString(String str, String substr) {
		//str = abcde  substr = abc  => true
		//str = abcde  substr = bcdea  => true
		//str = abcde  substr = ea  => true
		//str = abcde  substr = ec  => false
		//str = abcde  substr = abce  => false
		//str = abcde  substr = eabcdeab  => true
				
		//validation := length of substr > 2*length of str 
		return false;
	} 
	
